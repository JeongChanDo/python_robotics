    "import matplotlib.mlab as mlab\n",
    "import math\n",
    "import scipy.stats\n",
    "\n",
    "mu = 0\n",
    "variance = 5\n",
    "sigma = math.sqrt(variance)\n",
    "x = np.linspace(mu - 5*sigma, mu + 5*sigma, 100)\n",
    "plt.plot(x,scipy.stats.norm.pdf(x, mu, sigma))\n",
    "plt.show()"
   ]
